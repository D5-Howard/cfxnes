#emulator-buttons
    margin-left 24px

#emulator-canvas
    border 1px solid black
    margin auto
    position absolute
    top 0px
    bottom 0px
    left 0px
    right 0px

:-webkit-full-screen
    width 100%
    height 100%

:-moz-full-screen
    width 100%
    height 100%

:-ms-full-screen
    width 100%
    height 100%

:fullscreen
    width 100%
    height 100%

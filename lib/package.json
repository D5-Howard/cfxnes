{
  "name": "cfxnes",
  "version": "0.6.0",
  "description": "Library for NES emulation",
  "license": "MIT",
  "private": true,
  "keywords": [
    "emulator",
    "nes"
  ],
  "author": "Jan Pikl <pikl.jan@gmail.com>",
  "homepage": "https://github.com/jpikl/cfxnes",
  "bugs": {
    "url": "https://github.com/jpikl/cfxnes/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpikl/cfxnes"
  },
  "files": [
    "dist/",
    "API.md"
  ],
  "main": "dist/cfxnes.js",
  "scripts": {
    "dev": "npm run watch:debug",
    "build": "node build.js",
    "build:debug": "node build.js --debug",
    "watch": "watch 'npm run build' src ../core/src",
    "watch:debug": "watch 'npm run build:debug' src ../core/src",
    "lint": "eslint --report-unused-disable-directives src test \"*.js\"",
    "test": "karma start",
    "test:src": "karma start --target=src",
    "test:lib": "karma start --target=lib",
    "test:lib:debug": "karma start --target=lib:debug",
    "clean": "rimraf dist",
    "pretest": "npm run build",
    "pretest:lib": "npm run build",
    "pretest:lib:debug": "npm run build:debug"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "core-js": "^2.5.1",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "google-closure-compiler": "^20170521.0.0",
    "ip": "^1.1.5",
    "jszip": "^3.1.3",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chai-plugins": "^0.9.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webdriver-launcher": "^1.0.5",
    "mocha": "^4.0.0",
    "rimraf": "^2.6.2",
    "watch": "^1.0.2"
  }
}
